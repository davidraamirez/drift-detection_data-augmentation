   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "\n",
    "time = np.arange(0, 365)\n",
    "seasonal = 10 * np.sin(2 * np.pi * time / 365)\n",
    "noise = np.random.normal(0, 1, len(time))\n",
    "temperature = seasonal + noise\n",
    "\n",
    "df = pd.DataFrame({'Day': time, 'Temperature': temperature})"
   ]
